    | relational_expression \>  shift_expression
                                                    f:r { { type: env.typ.RelationalExpression, symbol:$$2, nodes:[$$1, $$3], pos } }
    | relational_expression \<= shift_expression
                                                    f:r { { type: env.typ.RelationalExpression, symbol:$$2, nodes:[$$1, $$3], pos } }
    | relational_expression \>= shift_expression
                                                    f:r { { type: env.typ.RelationalExpression, symbol:$$2, nodes:[$$1, $$3], pos } }
