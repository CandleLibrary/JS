{
  "name": "@candlefw/js",
  "version": "0.0.10",
  "description": "ECMASCRIPT Parser",
  "main": "source/ecma.mjs",
  "type": "module",
  "scripts": {
    "test": "cfw_test test/test.js",
    "test.watch": "mocha test/test.js -r esm --watch",
    "build": "rollup --config ./build/config/config.js",
    "build.watch": "rollup --config  ./build/config/config.js --watch",
    "build.ui.watch": "rollup --config  ./build/config/ui.config.js --watch",
    "compile": "hydrocarbon compile --parser glalr --unattended -u -o ./source/parser -e ./source/parser/env.mjs -t mjs ./source/grammar/ecmascript.hcg",
    "compile.test": "hydrocarbon compile --unattended -d -u -e ./source/parser/env.mjs -t mjs ./source/grammar/ecmascript.hcg",
    "compile.mount": "hydrocarbon compile -d -e ./source/parser/env.mjs -m ./source/grammar/ecmascript.hcg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/candlefw/js.git"
  },
  "keywords": [
    "ECMASCRIPT",
    "ecma",
    "javascript",
    "Parser"
  ],
  "author": "Anthony Weathersby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/candlefw/js/issues"
  },
  "homepage": "https://github.com/candlefw/js#readme",
  "dependencies": {
    "@candlefw/url": "latest",
    "@candlefw/whind": "latest"
  },
  "devDependencies": {
    "@candlefw/hydrocarbon": "latest",
    "chai": "^4.2.0",
    "esm": "^3.0.84",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "rollup": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-gzip": "latest",
    "rollup-plugin-node-resolve": "latest",
    "rollup-plugin-resolve": "latest",
    "rollup-plugin-terser": "latest"
  },
  "jshintConfig": {
    "esversion": 6,
    "unused": true
  }
}
