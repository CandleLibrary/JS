{
    "name": "@candlelib/js",
    "version": "0.5.2",
    "description": "Javascript Parser",
    "main": "./build/library/javascript.js",
    "types": "./build/types/javascript.d.ts",
    "type": "module",
    "scripts": {
        "prepublishOnly": "rm -rf ./build && tsc & sleep 10",
        "test": "candle.cure ./test/",
        "test.watch": "candle.cure -w ./test/candlelibrary.js.spec.js",
        "build": "tsc",
        "build.watch": "rollup --config  ./build/config/config.js --watch",
        "build.ui.watch": "rollup --config  ./build/config/ui.config.js --watch",
        "compile.debug": "hydrocarbon compile --workers 10 --output_dir ./source/typescript/parser/ --annotations --debug  ./source/grammar/javascript.hcg",
        "compile.release": "hydrocarbon compile --workers 10 --output_dir ./source/typescript/parser/ --recognizer_type wasm  ./source/grammar/javascript.hcg"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/candlelibrary/js.git"
    },
    "keywords": [
        "ECMASCRIPT",
        "ecma",
        "javascript",
        "Parser"
    ],
    "author": "Anthony Weathersby",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/candlelibrary/js/issues"
    },
    "homepage": "https://github.com/candlelibrary/js#readme",
    "dependencies": {
        "@assemblyscript/loader": "^0.17.13",
        "@candlelib/conflagrate": "0.4.1",
        "@candlelib/hydrocarbon": "0.11.0",
        "@candlelib/uri": "0.2.2",
        "@candlelib/wind": "0.5.1"
    },
    "devDependencies": {
        "@candlelib/cure": "0.5.0"
    },
    "jshintConfig": {
        "esversion": 6,
        "unused": true
    }
}